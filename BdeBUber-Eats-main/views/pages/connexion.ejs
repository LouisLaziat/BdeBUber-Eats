  <body class="text-center">
    <main class="form-signin w-100 m-auto">
      <p class="mb-4">
        BdebUber
        <a href="/" rel="noopener noreferrer" style="text-decoration: none"
          ><span class="p-span-eats">Eats</span></a
        >
      </p>

      <h1 class="h3 mb-3 fw-normal">Entrez vos informations</h1>

      <div class="form-floating">
        <input
          type="email"
          class="form-control"
          id="floatingInput"
          placeholder="name@example.com"
        />
        <label for="floatingInput">Adresse courriel</label>
      </div>
      <div class="form-floating">
        <input
          type="password"
          class="form-control"
          id="floatingPassword"
          placeholder="Password"
        />
        <label for="floatingPassword">Mot de passe</label>
      </div>
      <button
        class="w-100 btn btn-lg btn-sign-in"
        onclick="executeConnection()"
      >
        Se connecter
      </button>
      <br /><br />
      <p>
        <a href="inscription" class="a-inscription-link">S'inscrire</a>
      </p>
      <p id="Error" style="color: red"></p>
      <p class="mt-5 mb-3 text-muted">
        &copy; Tout droits reservés équipe JYGL BdeB 2023
      </p>
    </main>
    <script src="../../public/js/connexion.js"></script>
  </body>
